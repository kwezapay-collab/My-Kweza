{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(index|dashboard|settings|payouts-history|dev-operations).html",
            "dest": "/public/$1.html"
        },
        {
            "src": "/(index.css|logo.png|.*\\.js)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ]
}
