{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/(index|dashboard|settings|weekly-report|founder-weekly-reports|payouts-history|withdrawals-history|financial-withdrawals-history|dev-operations|complaints-history|super-admin).html",
            "dest": "/public/$1.html"
        },
        {
            "src": "/(index.css|logo.png|manifest.webmanifest|icons/.*|.*\\.js)",
            "dest": "/public/$1"
        },
        {
            "src": "/\\.well-known/assetlinks\\.json",
            "dest": "/public/.well-known/assetlinks.json"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ]
}
